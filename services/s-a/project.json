      "options": {}
    },
    "update": {
      "executor": "@nx-python-poetry/nx-python:update",
      "options": {}
    },
    "build": {
      "executor": "@nx-python-poetry/nx-python:build",
      "options": {
        "outputPath": "dist",
        "ignorePaths": []
      }
    }
  }
}
